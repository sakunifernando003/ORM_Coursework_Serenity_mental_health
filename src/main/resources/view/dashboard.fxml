         <children>
            <HBox fx:id="navBar" alignment="CENTER" prefHeight="600.0" prefWidth="800.0">
               <children>
                  <Button fx:id="navBarBtnPatient" mnemonicParsing="false" onAction="#navigateToPatientPage" style="-fx-background-color: #1E90FF;" text="Patient" textFill="WHITE">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="navBarBtnRegistration" layoutX="154.0" layoutY="24.0" mnemonicParsing="false" onAction="#navigateToRegistrationPage" style="-fx-background-color: #1E90FF;" text="Registration" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="navBarBtnTherapist" layoutX="206.0" layoutY="24.0" mnemonicParsing="false" onAction="#navigateToTherapistPage" style="-fx-background-color: #1E90FF;" text="Therapist" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="navBarBtnPrograms" layoutX="362.0" layoutY="24.0" mnemonicParsing="false" onAction="#navigateToProgramsPage" style="-fx-background-color: #1E90FF;" text="Programs" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="navBarBtnPayment" layoutX="310.0" layoutY="24.0" mnemonicParsing="false" onAction="#navigateToPamentPage" style="-fx-background-color: #1E90FF;" text="Payment" textFill="WHITE">
