                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="navBarBtnUSer" layoutX="414.0" layoutY="24.0" mnemonicParsing="false" onAction="#navigateToUserPage" prefHeight="25.0" prefWidth="66.0" style="-fx-background-color: #8c0d23;" text="exit" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
